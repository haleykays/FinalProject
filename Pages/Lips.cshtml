@page 
@model LipsModel

@{
    ViewData["Title"] = "Lips page";
}

<table class="table table-dark">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.lipProduct[0].Brand)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.lipProduct[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.lipProduct[0].Price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.lipProduct[0].BuyAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.lipProduct[0].Reviews)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.lipProduct) 
{
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Brand)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BuyAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Reviews)
            </td>
            <td>
            </td>
        </tr>
}
    </tbody>
</table>